{
  "name": "@oriflame/config-eslint",
  "version": "5.7.7",
  "description": "Reusable ESLint config.",
  "keywords": [
    "lumos",
    "config",
    "eslint"
  ],
  "main": "./lib/index.js",
  "types": "./dts/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git@github.com:Oriflame/lumos.git"
  },
  "author": "Silhan Jan",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=12.17.0",
    "npm": ">=6.13.0"
  },
  "files": [
    "dts/**/*.d.ts",
    "lib/**/*.{js,map,d.ts}",
    "lib/**/*.{js,map}",
    "src/**/*.{ts,tsx,json}"
  ],
  "dependencies": {
    "@oriflame/lumos-common": "^5.7.7",
    "eslint-config-oriflame": "^5.7.7"
  },
  "devDependencies": {
    "@beemo/core": "^2.1.4",
    "@beemo/driver-eslint": "^2.0.5",
    "@types/node": "^16.11.26",
    "eslint": "^8.11.0"
  },
  "peerDependencies": {
    "@beemo/core": "^2.1.2",
    "@beemo/driver-eslint": "^2.0.3",
    "eslint": "^8.4.1",
    "prettier": "^2.5.1",
    "typescript": ">=4.5"
  },
  "peerDependenciesMeta": {
    "prettier": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "packemon": {
    "format": "lib",
    "platform": "node"
  }
}
