    "eslint": "^8.4.1",
    "ts-node": "^10.4.0"
  },
  "peerDependencies": {
    "@beemo/core": "^2.1.2",
    "@beemo/driver-eslint": "^2.0.3",
    "eslint": "^8.4.1"
