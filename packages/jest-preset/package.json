{
  "name": "@oriflame/jest-preset",
  "version": "5.15.15",
  "description": "Jest preset for lumos configured projects.",
  "keywords": [
    "jest-preset",
    "oriflame",
    "lumos"
  ],
  "main": "./lib/index.js",
  "types": "./dts/index.d.ts",
  "files": [
    "dts/**/*.d.ts",
    "jest-preset.js",
    "lib/**/*.{js,map}",
    "src/**/*.{ts,tsx,json}"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:oriflame/lumos.git",
    "directory": "packages/jest-preset"
  },
  "author": "Jan Silhan",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14.15.0",
    "npm": ">=6.14.0"
  },
  "peerDependencies": {
    "jest": "^28.0.0 || ^29.0.0"
  },
  "dependencies": {
    "@jest/types": "^29.6.3",
    "@oriflame/lumos-common": "^5.15.15",
    "@types/jest": "^29.5.12",
    "jest-junit": "^14.0.0"
  },
  "devDependencies": {
    "jest": "^29.7.0"
  },
  "packemon": {
    "inputs": {
      "index": "src/index.ts",
      "fileMock": "src/fileMock.ts"
    },
    "format": "lib",
    "platform": "node"
  }
}
