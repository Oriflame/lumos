  "description": "Reusable Babel config.",
  "keywords": [
    "lumos",
    "config",
    "babel"
  ],
  "main": "./lib/index.js",
  "types": "./dts/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git@github.com:Oriflame/lumos.git"
  },
