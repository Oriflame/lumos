    "eslint": "^8.4.1",
    "execa": "^5.1.1",
    "jest": "^27.4.5",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
